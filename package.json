{
  "name": "@baleada/markdown-it-spa-links",
  "version": "0.7.1",
  "description": "Markdown-it plugin to replace links in an SPA with the proper components",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    "require": "./lib/index.cjs",
    "import": "./lib/index.js"
  },
  "scripts": {
    "prepare": "tsc && rollup --config rollup.config.ts",
    "test:only": "run() { uvu -r esbuild-register tests/$2 $1.test.ts$; }; run",
    "test": "npm run test:only ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baleada/markdown-it-spa-links.git"
  },
  "keywords": [
    "baleada",
    "markdown",
    "SPA links",
    "vue",
    "nuxt",
    "next",
    "sapper",
    "inertia",
    "markdown-it plugin"
  ],
  "author": {
    "name": "Alex Vipond",
    "email": "hello@alexvipond.dev",
    "url": "https://alexvipond.dev"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baleada/markdown-it-spa-links/issues"
  },
  "homepage": "https://baleada.netlify.com",
  "devDependencies": {
    "@baleada/prepare": "^0.5.12",
    "esbuild": "^0.12.26",
    "esbuild-register": "^3.0.0",
    "markdown-it-link-attributes": "^3.0.0",
    "rollup": "^2.35.1",
    "typescript": "^4.4.3",
    "uvu": "^0.5.1"
  },
  "dependencies": {
    "markdown-it": "^12.2.0"
  }
}
